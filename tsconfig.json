{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"declaration": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"jsx": "react-native",
		"lib": ["es2017", "dom"],
		"types": ["react"],
		"moduleResolution": "node",
		"noEmit": false,
		"strict": true,
		"target": "esnext"
	},
	"include": ["./"],
	"exclude": [
		"node_modules"
	]
}